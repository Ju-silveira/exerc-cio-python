n = str(input('digite augo :'))
print(f'é numerico {n.isnumeric()}')
print(f'é alfabetico {n.isalpha()}')
print(f'esta em letra minuscula {n.islower()}')
print(f'esta em letra maiuscula {n.isupper()}')
print(f'é numerico ou alfabetico {n.isalnum()}')
print(f'é apenas digitos {n.isdigit()}')
print(f'é apenas desimal {n.isdecimal()}')
print(f'tem um emdentificador valido {n.isidentifier()}')
print(f'pode ser inprimido {n.isprintable()}')
print(f'tem apenas espaso {n.isspace()}')
