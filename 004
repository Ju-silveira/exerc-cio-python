n = str(input('digite augo :'))
print(f'e numerico {n.isnumeric()}')
print(f'e alfabetico {n.isalpha()}')
print(f'esta em letra minuscula {n.islower()}')
print(f'esta em letra maiuscula {n.isupper()}')
print(f'e numerio ou aufabetico {n.isalnum()}')
print(f'e apenas digitos {n.isdigit()}')
print(f'e apenas desimal {n.isdecimal()}')
print(f'tem um emdentificador valido {n.isidentifier()}')
print(f'pode ser inprimedo {n.isprintable()}')
print(f'tem apenas espaso {n.isspace()}')
